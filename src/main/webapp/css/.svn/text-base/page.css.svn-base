/****** wizard progress style start ********/
#progress .step{
	float: left;
	border-right: 1px solid #CACACA;
}
#progress div .complete{
	background: none repeat scroll 0 0 #8CD0F0;
}
#progress div .parrow{
	background: url('../../images/re/signup-progress-arrow.png') no-repeat top right transparent;
}

/****** wizard setting check-box ***********/
.settings-checkbox {
	float: left;
    background: none repeat scroll 0 0 #F7F7F7;
    border: 1px solid #F7F7F7;
    margin-right: 4px;
    margin-top: 4px;
    height:30px;
    padding: 0 10px;
    cursor: pointer;
}
.settings-checkbox:hover {
    background: none repeat scroll 0 0 #DAF2FC;
    border-color: #DAF2FC;
}
.settings-checkbox.checked {
    background: none repeat scroll 0 0 #A2DEF8;
    border-color: #92CEE8;
    color: #333333;
}
.settings-checkbox label {
	cursor: pointer;
    font-size: 12px;
    line-height: 30px;
    font-weight: 500;
    white-space: nowrap;
}
.settings-checkbox input[type="checkbox"] {
	cursor: pointer;
	display: none;
}


.settings-checkbox.usetting {
	background: none;
	border: none;
	margin: 0px;
	height: auto;
	padding: 0px;
	color: #418EB5;
}
.settings-checkbox.usetting:hover {
	background:none;
	border-color: none;
	color:#8CD0F0;
}
.settings-checkbox.usetting.checked {
    background: none;
    border-color: none;
    color: #888888;
}
.settings-checkbox.usetting label {
	cursor: pointer;
    font-size: 12px;
    line-height: 15px;
    font-weight: 500;
    white-space: nowrap;
}
/****** tiny button: start***********/
.tinybtn {
    border: 1px solid #ccc;
    border-radius: 3px;
    padding-bottom: 4px;
    padding-left: 23px;
    padding-top: 3px;
}
.tinybtn:hover {
    background-color: #F0EEEC;
}


/****** tiny button: end***********/

