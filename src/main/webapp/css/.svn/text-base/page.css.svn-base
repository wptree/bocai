/****** more_at :start************/
.more_at{
	overflow: hidden;
}

.more_at #header {
	border-bottom: 1px solid #FFFFFF;
    padding-bottom: 5px;
    position: relative;
}

.more_at #title {
    float: left;
    color: #4D4544;
    font-size:13px;
    height: 20px;
    font-weight: bold;
    line-height: 20px;
    font-family: "Microsoft Yahei","微软雅黑","宋体",Verdana,Tahoma,Hei,Helvetica,Arial,sans-serif
}

.more_at #loading {
    background: url("../../images/widget-page-load.gif") no-repeat scroll center center #FFFFFF;
    height: 20px;
    position: absolute;
    right: 0px;
    width: 42px;
}

.more_at #controls {
    float: right;
}

.more_at #controls #next_page {
    background: url("../../images/widget-sprites.png") no-repeat scroll -20px -16px transparent;
    float: right;
    height: 20px;
    overflow: hidden;
    width: 20px;
}

.more_at #controls #prev_page {
    background: url("../../images/widget-sprites.png") no-repeat scroll 0 -16px transparent;
    float: right;
    height: 20px;
    margin-right: 2px;
    overflow: hidden;
    width: 20px;
}

.more_at #controls #prev_page.disabled {
    background-position: 0 -36px;
    cursor: default;
}

.more_at #controls #next_page.disabled {
    background-position: -20px -36px;
    cursor: default;
}

.more_at #controls #entries {
    float: right;
    line-height: 20px;
    margin-right: 10px;
}

.more_at #controls #entries #range{
	font-style: italic;
}

.more_at #sightings {
    display: inline-block;
    margin-top: 5px;
}

.more_at #sightings-wrap {
    overflow: hidden;
}

.more_at #sightings .sighting-set {
    float: left;
    position: relative;
}

/****** more_at :end************/

