/******* tool-tip :start***********/
#tooltip {
    position: absolute;
    z-index: 100001;
}

#tooltip .content {
    background: url("../../images/tooltip-content.png") repeat scroll 0 0 transparent;
    border-radius: 5px 5px 5px 5px;
    color: white;
    font-size: 0.9em;
    font-weight: bold;
    line-height: 1.2em;
    padding: 8px 10px;
}

#tooltip .pointer {
    background: url("../../images/tooltip-pointer.png") no-repeat scroll center top transparent;
    height: 5px;
    text-align: center;
    width: 100%;
}
/******* tool-tip :end***********/

