/*元素*/
a:hover{text-decoration: underline;}
input[type='text'],input[type='password'],textarea{-moz-border-radius: 5px;
     -webkit-border-radius: 5px;border-radius: 5px;border-style:solid;}
.opacity{background-color:black;opacity:0.1;filter:alpha(opacity=10);}
.border1pxddd{border:1px solid #ddd;}
.border1pxeee{border:1px solid #eee;}
.liNone{list-style-type:none;padding:0;margin:0;}
li.lioutside{list-style-position:outside;}

a.btnFollow{background: #41AAB9;border: none;box-shadow: inset -1px -1px #196473;text-decoration:none;}
a.btnFollow:hover{background-color: #268CA0;box-shadow: 1px 1px 1px #196473 inset;outline: 0 none;text-decoration:none;}
input.btnFollow{background: #41AAB9;border: none;box-shadow: inset -1px -1px #196473;text-decoration:none;font-family: "Microsoft Yahei","微软雅黑",Tahoma,Arial,Helvetica,STHeiti;}
input.btnFollow:hover{background-color: #268CA0;box-shadow: 1px 1px 1px #196473 inset;outline: 0 none;text-decoration:none;font-family: "Microsoft Yahei","微软雅黑",Tahoma,Arial,Helvetica,STHeiti;}
.boxShad{box-shadow: 1px 1px 3px #B4AAA0;-moz-box-shadow: 1px 1px 3px #B4AAA0;-webkit-box-shadow: 1px 1px 3px #B4AAA0;}
.boxShad1{box-shadow: inset 1px 1px 1px rgba(0, 0, 0, 0.2);-moz-box-shadow: inset 1px 1px 1px rgba(0, 0, 0, 0.2);-webkit-box-shadow: inset 1px 1px 1px rgb(0, 0, 0, 0.2);}
.boxShad2{box-shadow: 2px 0 4px rgba(0, 0, 0, 0.5);-moz-box-shadow: 2px 0 4px rgba(0, 0, 0, 0.5);-webkit-box-shadow: 2px 0 4px rgba(0, 0, 0, 0.5);}
.boxShadSpotImg{box-shadow: 1px 1px 3px #E1DCC8;}
.bbox { 
	background-color: #F7F6F1;
    border-radius: 5px 5px 5px 5px;
    box-shadow: 1px 1px 3px #B4AAA0;
    border-bottom: 1px solid #cfc5a9 \9;
	border-right: 1px solid #cfc5a9 \9;
}
.bbox-tabs{
	background-color: #F7F6F1;
    border-radius: 5px 5px 5px 5px;
    box-shadow: 1px 1px 3px #B4AAA0;
}
.arrow {
    font-size: 14px;
    height: 14px;
    line-height: 20px;
    margin: -21px 0 0;
    overflow: hidden;
    width: 25px;
}
.big-loading-box {
    background: url("../../images/tooltip-content.png") repeat scroll 0 0 transparent;
    border-radius: 5px 5px 5px 5px;
    font-size: 100%;
    font-weight: bold;
    height: 60px;
    left: 50%;
    margin: 0 0 0 -30px;
    position: absolute;
    text-align: center;
    width: 60px;
    z-index: 1000;
}
.big-loading-spinner {
    background: url("../../images/spinner-large.gif") no-repeat scroll 14px 14px transparent;
    height: 60px;
    opacity: 0.75;
    width: 60px;
}

.op7{opacity: 0.7;}
