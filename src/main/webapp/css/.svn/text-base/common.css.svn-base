a.tool{
	color: #FFFFFF;
	border: 1px solid #5C7E27;
	background-color: #7ec114;
	height:20px;
	line-height: 20px;
	padding-top: 3px;
	padding-bottom: 3px;
	padding-left: 25px;
	padding-right: 5px;
	border-radius: 3px;
	font-family: "Microsoft Yahei","微软雅黑",Tahoma,Arial,Helvetica,STHeiti;
	background-attachment: scroll;
	background-repeat: no-repeat;
	background-position: 5px center;
}
a.tool:hover{
	background-color:#9bdd34;
	text-decoration: none;
	cursor: pointer;
}
a.tool.msg{
	background-image: url("../../images/re/icon-msg.png");
}

