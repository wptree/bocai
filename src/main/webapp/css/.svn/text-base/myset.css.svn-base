}

.listdata {
	position: absolute;
	border: 1px solid #D4D4D2;
	width: 100px;
	height: auto;
	overflow: hidden;
	background-color: #FFFFFF;
}

.selectzhi {
	margin: 0;
	width: 90px;
	height: 20px;
	line-height: 100%;
	overflow: hidden;
	font-size: 12px;
	padding-right: 10px;
	padding-top: 4px;
	color: #416B00;
	background: url(../images/szxxzl1_14.jpg) no-repeat right 7px;
}

.listdata ul {
	margin: 0px;
	padding-left: 0px;
	width: 100px;
	height: auto;
	overflow: hidden;
}

.listdata ul li {
	list-style: none;
	padding-left: 3px;
	padding-top: 2px;
	font-size: 12px;
	height: 16px;
	width: 100px;
	padding-left: 3px;
	cursor: default;
}