div.token-input-dropdown-upload ul li b {
	color: #ff7070;
    font-weight: bold;
}

div.token-input-dropdown-upload ul li.token-input-selected-dropdown-item-upload {
	letter-spacing:2px;
	color: #fff;
    background-color: #50b7f2;
