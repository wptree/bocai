/* Example tokeninput style #2: upload style */
ul.token-input-list-upload {
}


ul.token-input-list-upload li input {
}

li.token-input-token-upload {
    background-color: #BCDD8E;
    border: 1px solid #FFFFFF;
    border-radius: 5px;
    color: #000000;
    cursor: default;
    font-size: 14px;
    line-height: 36px;
    overflow: hidden;
    padding: 0 6px;
    font-weight: bold;
}

li.token-input-token-upload p {
    display: inline;
    text-shadow: 0 1px 0 #F0EEEC;
    padding: 0;
    margin: 0;
}

li.token-input-token-upload span {
	color:#fff;
   	float: right;
   	font-size: 18px;
    cursor: pointer;
}

li.token-input-selected-token-upload {
    background-color: #57A957;
    border: 1px solid #fff;
    color: #fff;
}

li.token-input-selected-token-upload p{
    text-shadow: 0 1px 0 #000;
}

li.token-input-input-token-upload {}

div.token-input-dropdown-upload {
    position: absolute;
    background-color: #EBF7FF;
    overflow: hidden;
    border: 1px solid #A7D9FA;
    border-radius: 0 0 5px 5px;
    cursor: default;
    line-height: 14px;
    z-index: 1;
    color: #4d84b0;
    font-size: 12px;
    font-family: "Microsoft Yahei","Î¢ÈíÑÅºÚ",Tahoma,Arial,Helvetica,STHeiti;
}

div.token-input-dropdown-upload p {
	white-space:nowrap;
    padding: 5px;
}

.searching-loading {
	background: url("../images/loading.gif") no-repeat;
	width:16px;
	height:16px;
	float: left;
	margin-left: 2px;
	margin-right: 2px;
}
.first-line-create-upload{
	text-decoration:none;
	cursor:pointer;
    font-weight: bold;
}

div.token-input-dropdown-upload ul {
    margin: 0;
    padding: 0;
}

div.token-input-dropdown-upload ul li {
    padding: 5px;
    list-style-type: none;
}

